  pod 'KMPlaceholderTextView', '~> 1.3.0'

  target 'DiffusionTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'DiffusionUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
